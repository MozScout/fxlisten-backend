{
  "disableLanguages": [
    "vue"
  ],
  "eslintIntegration": false,
  "tslintIntegration": false,
  "stylelintIntegration": false,
  "requireConfig": false,
  "ignorePath": ".prettierignore",
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "parser": "babel",
  "semi": true,
  "useTabs": false,
  "proseWrap": "preserve",
  "arrowParens": "avoid",
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "auto",
}